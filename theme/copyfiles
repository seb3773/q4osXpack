user_length=${#USER_SU}
Xpos=$(( (Xres - (22 * user_length)) / 2 ))
Ypos=$(echo "$Yres * 0.473" | bc)
Ypos_rounded=$(awk 'BEGIN { printf "%.0f\n", '"$Ypos"' }')
Ypos2=$((Ypos_rounded + 20))
txpos () {
echo "Username Text Position $Xres = $Xpos,$Ypos_rounded" | sudo tee -a "/opt/trinity/share/apps/ksplash/Themes/Redmond10_$USER_SU/Theme.rc" 
echo "Action Text Position $Xres = $Xpos,$Ypos2" | sudo tee -a "/opt/trinity/share/apps/ksplash/Themes/Redmond10_$USER_SU/Theme.rc"
}
