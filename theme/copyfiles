echo "  ░▒▓█ copying cursors"
create_links() {
cd $1
ln -s half-busy 00000000000000020006000e7e9ffc3f > /dev/null 2>&1
ln -s half-busy 08e8e1c95fe2fc01f976f1e063a24ccd > /dev/null 2>&1
