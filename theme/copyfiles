#!/bin/sh
#check root
if [ "$(id -u)" -ne 0 ]; then
echo 'This script must be run by root' >&2
exit 1
fi
#
USER_HOME=$(eval echo ~${SUDO_USER})
USER_SU=$(eval echo ${SUDO_USER})
SEPL="════════════════════════════════════════════════════════════════════════════════════════"
osarch=$(dpkg --print-architecture)
echo "== Copying various files..."
echo "${SEPL}"
echo "  ░▒▓█ copying icons"
osx=0

if [ $1 -eq 1 ]
then
iconth="dark"
fi

if [ $1 -eq 0 ]
then
iconth="light"
fi

if [ $1 -eq 99 ]
then
osx=1
fi

codlang="$2"

if [ ! $osx -eq 1 ]; then
rm -fr /usr/share/icons/kdeten_$iconth > /dev/null 2>&1
